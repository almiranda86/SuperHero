# Docker Compose Environment Variables
# Copy this file to .env and adjust values as needed

# RabbitMQ Configuration
RABBITMQ_DEFAULT_USER=your_rabbitmq_user
RABBITMQ_DEFAULT_PASS=your_secure_rabbitmq_password

# MongoDB Configuration
MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=your_secure_mongodb_password
MONGO_INITDB_DATABASE=HeroesDB

# Mongo Express Configuration
MONGO_EXPRESS_PASSWORD=your_secure_mongoexpress_password

# Redis Configuration
REDIS_PASSWORD=

# Logging
log_rabbitmq_management=rabbitmq-logs
